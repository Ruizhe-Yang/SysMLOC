<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="Model">
  <eSubpackages name="SysML" nsURI="https://ww.omg.org/spec/SysML/20240201">
    <eSubpackages name="Systems">
      <eSubpackages name="Ports">
        <eClassifiers xsi:type="ecore:EClass" name="ConjugatedPortDefinition" eSuperTypes="#//SysML/Systems/Ports/PortDefinition">
          <eAnnotations source="&lt;p>A &lt;code>ConjugatedPortDefinition&lt;/code> is a &lt;code>PortDefinition&lt;/code> that is a &lt;code>PortDefinition&lt;/code> of its original &lt;code>PortDefinition&lt;/code>. That is, a &lt;code>ConjugatedPortDefinition&lt;/code> inherits all the &lt;code>features&lt;/code> of the original &lt;code>PortDefinition&lt;/code>, but input &lt;code>flows&lt;/code> of the original &lt;code>PortDefinition&lt;/code> become outputs on the &lt;code>ConjugatedPortDefinition&lt;/code> and output &lt;code>flows&lt;/code> of the original &lt;code>PortDefinition&lt;/code> become inputs on the &lt;code>ConjugatedPortDefinition&lt;/code>. Every &lt;code>PortDefinition&lt;/code> (that is not itself a &lt;code>&lt;code>ConjugatedPortDefinition&lt;/code>&lt;/code>) has exactly one corresponding &lt;code>ConjugatedPortDefinition&lt;/code>, whose effective name is the name of the &lt;code>originalPortDefinition&lt;/code>, with the character &lt;code>~&lt;/code> prepended.&lt;/p>"
              references="#//SysML/Systems/Ports/ConjugatedPortDefinition"/>
          <eOperations name="effectiveName">
            <eAnnotations source="&lt;p>If the &lt;code>name&lt;/code> of the &lt;code>originalPortDefinition&lt;/code> is non-empty, then return that with the character &lt;code>~&lt;/code> prepended.&lt;/p>"
                references="#//SysML/Systems/Ports/ConjugatedPortDefinition/effectiveName"/>
            <eParameters ordered="false"/>
          </eOperations>
          <eStructuralFeatures xsi:type="ecore:EReference" name="originalPortDefinition"
              ordered="false" lowerBound="1" eType="#//SysML/Systems/Ports/PortDefinition"
              derived="true" eOpposite="#//SysML/Systems/Ports/PortDefinition/conjugatedPortDefinition">
            <eAnnotations source="&lt;p>The original &lt;code>PortDefinition&lt;/code> for this &lt;code>ConjugatedPortDefinition&lt;/code>, which is the &lt;code>owningNamespace&lt;/code> of the &lt;code>ConjugatedPortDefinition&lt;/code>.&lt;/p>"
                references="#//SysML/Systems/Ports/ConjugatedPortDefinition/originalPortDefinition"/>
          </eStructuralFeatures>
          <eStructuralFeatures xsi:type="ecore:EReference" name="ownedPortConjugator"
              ordered="false" lowerBound="1" eType="#//SysML/Systems/Ports/PortConjugation"
              derived="true" eOpposite="#//SysML/Systems/Ports/PortConjugation/conjugatedPortDefinition">
            <eAnnotations source="&lt;p>The &lt;code>PortConjugation&lt;/code> that is the &lt;code>ownedConjugator&lt;/code> of this &lt;code>ConjugatedPortDefinition&lt;/code>, linking it to its &lt;code>originalPortDefinition&lt;/code>.&lt;/p>"
                references="#//SysML/Systems/Ports/ConjugatedPortDefinition/ownedPortConjugator"/>
          </eStructuralFeatures>
        </eClassifiers>
        <eClassifiers xsi:type="ecore:EClass" name="PortUsage" eSuperTypes="#//SysML/Systems/Occurrences/OccurrenceUsage">
          <eAnnotations source="&lt;p>A &lt;code>PortUsage&lt;/code> is a usage of a &lt;code>PortDefinition&lt;/code>. A &lt;code>PortUsage&lt;/code> itself as well as all its &lt;code>nestedUsages&lt;/code> must be referential (non-composite).&lt;/p>"
              references="#//SysML/Systems/Ports/PortUsage"/>
          <eStructuralFeatures xsi:type="ecore:EReference" name="portDefinition" upperBound="-1"
              eType="#//SysML/Systems/Ports/PortDefinition" derived="true">
            <eAnnotations source="&lt;p>The &lt;code>occurrenceDefinitions&lt;/code> of this &lt;code>PortUsage&lt;/code>, which must all be &lt;code>PortDefinitions&lt;code>.&lt;/p>"
                references="#//SysML/Systems/Ports/PortUsage/portDefinition"/>
          </eStructuralFeatures>
        </eClassifiers>
        <eClassifiers xsi:type="ecore:EClass" name="ConjugatedPortTyping">
          <eAnnotations source="&lt;p>A &lt;code>ConjugatedPortTyping&lt;/code> is a &lt;code>FeatureTyping&lt;/code> whose &lt;code>type&lt;/code> is a &lt;code>ConjugatedPortDefinition&lt;/code>. (This relationship is intended to be an abstract-syntax marker for a special surface notation for conjugated typing of ports.)&lt;/p>"
              references="#//SysML/Systems/Ports/ConjugatedPortTyping"/>
          <eStructuralFeatures xsi:type="ecore:EReference" name="portDefinition" ordered="false"
              lowerBound="1" eType="#//SysML/Systems/Ports/PortDefinition" derived="true">
            <eAnnotations source="&lt;p>The &lt;code>originalPortDefinition&lt;/code> of the &lt;code>conjugatedPortDefinition&lt;/code> of this &lt;code>ConjugatedPortTyping&lt;/code>.&lt;/p>"
                references="#//SysML/Systems/Ports/ConjugatedPortTyping/portDefinition"/>
          </eStructuralFeatures>
          <eStructuralFeatures xsi:type="ecore:EReference" name="conjugatedPortDefinition"
              ordered="false" lowerBound="1" eType="#//SysML/Systems/Ports/ConjugatedPortDefinition">
            <eAnnotations source="&lt;p>The &lt;code>type&lt;/code> of this &lt;code>ConjugatedPortTyping&lt;/code> considered as a &lt;code>FeatureTyping&lt;/code>, which must be a &lt;code>ConjugatedPortDefinition&lt;/code>.&lt;/p> "
                references="#//SysML/Systems/Ports/ConjugatedPortTyping/conjugatedPortDefinition"/>
          </eStructuralFeatures>
        </eClassifiers>
        <eClassifiers xsi:type="ecore:EClass" name="PortDefinition" eSuperTypes="#//SysML/Systems/Occurrences/OccurrenceDefinition">
          <eAnnotations source="&lt;p>A &lt;code>PortDefinition&lt;/code> defines a point at which external entities can connect to and interact with a system or part of a system. Any &lt;code>ownedUsages&lt;/code> of a &lt;code>PortDefinition&lt;/code>, other than &lt;code>PortUsages&lt;/code>, must not be composite.&lt;/p>   "
              references="#//SysML/Systems/Ports/PortDefinition"/>
          <eStructuralFeatures xsi:type="ecore:EReference" name="conjugatedPortDefinition"
              ordered="false" eType="#//SysML/Systems/Ports/ConjugatedPortDefinition"
              derived="true" eOpposite="#//SysML/Systems/Ports/ConjugatedPortDefinition/originalPortDefinition">
            <eAnnotations source="&lt;p>The &lt;codeConjugatedPortDefinition&lt;/code> that is conjugate to this &lt;code>PortDefinition&lt;/code>.&lt;/p>"
                references="#//SysML/Systems/Ports/PortDefinition/conjugatedPortDefinition"/>
          </eStructuralFeatures>
        </eClassifiers>
        <eClassifiers xsi:type="ecore:EClass" name="PortConjugation">
          <eAnnotations source="&lt;p>A &lt;code>PortConjugation&lt;/code> is a &lt;code>Conjugation&lt;/code> &lt;code>Relationship&lt;/code> between a &lt;code>PortDefinition&lt;/code> and its corresponding &lt;code>ConjugatedPortDefinition&lt;/code>. As a result of this &lt;code>Relationship&lt;/code>, the &lt;code>ConjugatedPortDefinition&lt;/code> inherits all the &lt;code>features&lt;/code> of the original &lt;code>PortDefinition&lt;/code>, but input &lt;code>flows&lt;/code> of the original &lt;code>PortDefinition&lt;/code> become outputs on the &lt;code>ConjugatedPortDefinition&lt;/code> and output &lt;code>flows&lt;/code> of the original &lt;code>PortDefinition&lt;/code> become inputs on the &lt;code>ConjugatedPortDefinition&lt;/code>.&lt;/code>&lt;/p> "
              references="#//SysML/Systems/Ports/PortConjugation"/>
          <eStructuralFeatures xsi:type="ecore:EReference" name="originalPortDefinition"
              ordered="false" lowerBound="1" eType="#//SysML/Systems/Ports/PortDefinition">
            <eAnnotations source="&lt;p>The &lt;code>PortDefinition&lt;/code> being conjugated.&lt;/p>"
                references="#//SysML/Systems/Ports/PortConjugation/originalPortDefinition"/>
          </eStructuralFeatures>
          <eStructuralFeatures xsi:type="ecore:EReference" name="conjugatedPortDefinition"
              ordered="false" lowerBound="1" eType="#//SysML/Systems/Ports/ConjugatedPortDefinition"
              derived="true" eOpposite="#//SysML/Systems/Ports/ConjugatedPortDefinition/ownedPortConjugator">
            <eAnnotations source="&lt;p>The &lt;code>ConjugatedPortDefinition&lt;/code> that is conjugate to the &lt;code>originalPortDefinition&lt;/code>.&lt;/p>"
                references="#//SysML/Systems/Ports/PortConjugation/conjugatedPortDefinition"/>
          </eStructuralFeatures>
        </eClassifiers>
      </eSubpackages>
      <eSubpackages name="UseCases">
        <eClassifiers xsi:type="ecore:EClass" name="UseCaseUsage" eSuperTypes="#//SysML/Systems/Cases/CaseUsage">
          <eAnnotations source="&lt;p>A &lt;code>UseCaseUsage&lt;/code> is a &lt;code>Usage&lt;/code> of a &lt;code>UseCaseDefinition&lt;/code>.&lt;/p>"
              references="#//SysML/Systems/UseCases/UseCaseUsage"/>
          <eStructuralFeatures xsi:type="ecore:EReference" name="useCaseDefinition"
              ordered="false" eType="#//SysML/Systems/UseCases/UseCaseDefinition"
              derived="true">
            <eAnnotations source="&lt;p>The &lt;code>UseCaseDefinition&lt;/code> that is the &lt;code>definition&lt;/code> of this &lt;code>UseCaseUsage&lt;/code>.&lt;/p>"
                references="#//SysML/Systems/UseCases/UseCaseUsage/useCaseDefinition"/>
          </eStructuralFeatures>
          <eStructuralFeatures xsi:type="ecore:EReference" name="includedUseCase"
              upperBound="-1" eType="#//SysML/Systems/UseCases/UseCaseUsage" derived="true">
            <eAnnotations source="&lt;p>The &lt;code>UseCaseUsages&lt;/code> that are included by this &lt;code>UseCaseUse&lt;/code>, which are the &lt;code>useCaseIncludeds&lt;/code> of the &lt;code>IncludeUseCaseUsages&lt;/code> owned by this &lt;code>UseCaseUsage&lt;code>.&lt;/p>"
                references="#//SysML/Systems/UseCases/UseCaseUsage/includedUseCase"/>
          </eStructuralFeatures>
        </eClassifiers>
        <eClassifiers xsi:type="ecore:EClass" name="UseCaseDefinition" eSuperTypes="#//SysML/Systems/Cases/CaseDefinition">
          <eAnnotations source="&lt;p>A &lt;code>UseCaseDefinition&lt;/code> is a &lt;code>CaseDefinition&lt;/code> that specifies a set of actions performed by its subject, in interaction with one or more actors external to the subject. The objective is to yield an observable result that is of value to one or more of the actors.&lt;/p> "
              references="#//SysML/Systems/UseCases/UseCaseDefinition"/>
          <eStructuralFeatures xsi:type="ecore:EReference" name="includedUseCase"
              upperBound="-1" eType="#//SysML/Systems/UseCases/UseCaseUsage" derived="true">
            <eAnnotations source="&lt;p>The &lt;code>UseCaseUsages&lt;/code> that are included by this &lt;code>UseCaseDefinition&lt;/code>, which are the &lt;code>useCaseIncludeds&lt;/code> of the &lt;code>IncludeUseCaseUsages&lt;/code> owned by this &lt;code>UseCaseDefinition&lt;code>.&lt;/p>"
                references="#//SysML/Systems/UseCases/UseCaseDefinition/includedUseCase"/>
          </eStructuralFeatures>
        </eClassifiers>
        <eClassifiers xsi:type="ecore:EClass" name="IncludeUseCaseUsage" eSuperTypes="#//SysML/Systems/Actions/PerformActionUsage #//SysML/Systems/UseCases/UseCaseUsage">
          <eAnnotations source="&lt;p>An &lt;code>IncludeUseCaseUsage&lt;/code> is a &lt;code>UseCaseUsage&lt;/code> that represents the inclusion of a &lt;code>UseCaseUsage&lt;/code> by a &lt;code>UseCaseDefinition&lt;/code> or &lt;code>UseCaseUsage&lt;/code>. Unless it is the &lt;code>IncludeUseCaseUsage&lt;/code> itself, the &lt;code>UseCaseUsage&lt;/code> to be included is related to the &lt;code>includedUseCase&lt;/code> by a &lt;code>ReferenceSubsetting&lt;/code> &lt;code>Relationship&lt;/code>. An &lt;code>IncludeUseCaseUsage&lt;/code> is also a PerformActionUsage, with its &lt;code>useCaseIncluded&lt;/code> as the &lt;code>performedAction&lt;/code>.&lt;/p> "
              references="#//SysML/Systems/UseCases/IncludeUseCaseUsage"/>
          <eStructuralFeatures xsi:type="ecore:EReference" name="useCaseIncluded"
              ordered="false" lowerBound="1" eType="#//SysML/Systems/UseCases/UseCaseUsage"
              derived="true">
            <eAnnotations source="&lt;p>The &lt;code>UseCaseUsage&lt;/code> to be included by this &lt;code>IncludeUseCaseUsage&lt;/code>. It is the &lt;code>performedAction&lt;/code> of the &lt;code>IncludeUseCaseUsage&lt;/code> considered as a &lt;code>PerformActionUsage&lt;/code>, which must be a &lt;code>UseCaseUsage&lt;/code>.&lt;/p>  "
                references="#//SysML/Systems/UseCases/IncludeUseCaseUsage/useCaseIncluded"/>
          </eStructuralFeatures>
        </eClassifiers>
      </eSubpackages>
      <eSubpackages name="DefinitionAndUsage">
        <eClassifiers xsi:type="ecore:EClass" name="Usage">
          <eAnnotations source="&lt;p>A &lt;code>Usage&lt;/code> is a usage of a &lt;code>Definition&lt;/code>. A &lt;code>Usage&lt;/code> may only be an &lt;code>ownedFeature&lt;/code> of a &lt;code>Definition&lt;/code> or another &lt;code>Usage&lt;/code>.&lt;/p>  &lt;p>A &lt;code>Usage&lt;/code> may have &lt;code>nestedUsages&lt;/code> that model &lt;code>features&lt;/code> that apply in the context of the &lt;code>owningUsage&lt;/code>. A &lt;code>Usage&lt;/code> may also have &lt;code>Definitions&lt;/code> nested in it, but this has no semantic significance, other than the nested scoping resulting from the &lt;code>Usage&lt;/code> being considered as a &lt;code>Namespace&lt;/code> for any nested &lt;code>Definitions&lt;/code>.&lt;/p>  &lt;p>However, if a &lt;code>Usage&lt;/code> has &lt;code>isVariation = true&lt;/code>, then it represents a &lt;em>variation point&lt;/em> &lt;code>Usage&lt;/code>. In this case, all of its &lt;code>members&lt;/code> must be &lt;code>variant&lt;/code> &lt;code>Usages&lt;/code>, related to the &lt;code>Usage&lt;/code> by &lt;code>VariantMembership&lt;/code> &lt;code>Relationships&lt;/code>. Rather than being &lt;code>features&lt;/code> of the &lt;code>Usage&lt;/code>, &lt;code>variant&lt;/code> &lt;code>Usages&lt;/code> model different concrete alternatives that can be chosen to fill in for the variation point &lt;code>Usage&lt;/code>.&lt;/p>"
              references="#//SysML/Systems/DefinitionAndUsage/Usage"/>
          <eOperations name="namingFeature">
            <eAnnotations source="&lt;p>If this &lt;code>Usage&lt;/code> is a variant, then its naming &lt;code>Feature&lt;/code> is the &lt;code>referencedFeature&lt;/code> of its &lt;code>ownedReferenceSubsetting&lt;/code>.&lt;/p>"
                references="#//SysML/Systems/DefinitionAndUsage/Usage/namingFeature"/>
            <eParameters ordered="false"/>
          </eOperations>
          <eStructuralFeatures xsi:type="ecore:EAttribute" name="isReference" ordered="false"
              lowerBound="1" derived="true">
            <eAnnotations source="&lt;p>Whether this &lt;code>Usage&lt;/code> is a referential &lt;code>Usage&lt;/code>, that is, it has &lt;code>isComposite = false&lt;/code>.&lt;p>"
                references="#//SysML/Systems/DefinitionAndUsage/Usage/isReference"/>
          </eStructuralFeatures>
          <eStructuralFeatures xsi:type="ecore:EAttribute" name="isVariation" ordered="false"
              lowerBound="1">
            <eAnnotations source="&lt;p>Whether this &lt;code>Usage&lt;/code> is for a variation point or not. If true, then all the &lt;code>memberships&lt;/code> of the &lt;code>Usage&lt;/code> must be &lt;code>VariantMemberships&lt;/code>.&lt;/p>"
                references="#//SysML/Systems/DefinitionAndUsage/Usage/isVariation"/>
          </eStructuralFeatures>
          <eStructuralFeatures xsi:type="ecore:EReference" name="variant" ordered="false"
              upperBound="-1" eType="#//SysML/Systems/DefinitionAndUsage/Usage" derived="true">
            <eAnnotations source="&lt;p>The &lt;code>Usages&lt;/code> which represent the variants of this &lt;code>Usage&lt;/code> as a variation point &lt;code>Usage&lt;/code>, if &lt;code>isVariation = true&lt;/code>. If &lt;code>isVariation = false&lt;/code>, then there must be no &lt;code>variants&lt;/code>.&lt;/p>"
                references="#//SysML/Systems/DefinitionAndUsage/Usage/variant"/>
          </eStructuralFeatures>
          <eStructuralFeatures xsi:type="ecore:EReference" name="variantMembership"
              ordered="false" upperBound="-1" eType="#//SysML/Systems/DefinitionAndUsage/VariantMembership"
              derived="true">
            <eAnnotations source="&lt;p>The &lt;code>ownedMemberships&lt;/code> of this &lt;code>Usage&lt;/code> that are &lt;code>VariantMemberships&lt;/code>. If &lt;code>isVariation = true&lt;/code>, then this must be all &lt;code>memberships&lt;/code> of the &lt;code>Usage&lt;/code>. If &lt;code>isVariation = false&lt;/code>, then &lt;code>variantMembership&lt;/code>must be empty.&lt;/p>"
                references="#//SysML/Systems/DefinitionAndUsage/Usage/variantMembership"/>
          </eStructuralFeatures>
          <eStructuralFeatures xsi:type="ecore:EReference" name="owningDefinition"
              ordered="false" eType="#//SysML/Systems/DefinitionAndUsage/Definition"
              derived="true" eOpposite="#//SysML/Systems/DefinitionAndUsage/Definition/ownedUsage">
            <eAnnotations source="&lt;p>The &lt;code>Definition&lt;/code> that owns this &lt;code>Usage&lt;/code> (if any).&lt;/p>"
                references="#//SysML/Systems/DefinitionAndUsage/Usage/owningDefinition"/>
          </eStructuralFeatures>
          <eStructuralFeatures xsi:type="ecore:EReference" name="owningUsage" ordered="false"
              eType="#//SysML/Systems/DefinitionAndUsage/Usage" derived="true" eOpposite="#//SysML/Systems/DefinitionAndUsage/Usage/nestedUsage">
            <eAnnotations source="&lt;p>The &lt;code>Usage&lt;/code> in which this &lt;code>Usage&lt;/code> is nested (if any).&lt;/p>"
                references="#//SysML/Systems/DefinitionAndUsage/Usage/owningUsage"/>
          </eStructuralFeatures>
          <eStructuralFeatures xsi:type="ecore:EReference" name="usage" upperBound="-1"
              eType="#//SysML/Systems/DefinitionAndUsage/Usage" derived="true">
            <eAnnotations source="&lt;p>The &lt;code>Usages&lt;/code> that are &lt;code>features&lt;/code> of this &lt;code>Usage&lt;/code> (not necessarily owned).&lt;/p>"
                references="#//SysML/Systems/DefinitionAndUsage/Usage/usage"/>
            <eAnnotations/>
          </eStructuralFeatures>
          <eStructuralFeatures xsi:type="ecore:EReference" name="directedUsage" upperBound="-1"
              eType="#//SysML/Systems/DefinitionAndUsage/Usage" derived="true">
            <eAnnotations source="&lt;p>The &lt;code>usages&lt;/code> of this &lt;code>Usage&lt;/code> that are &lt;code>directedFeatures&lt;/code>.&lt;/p> "
                references="#//SysML/Systems/DefinitionAndUsage/Usage/directedUsage"/>
          </eStructuralFeatures>
          <eStructuralFeatures xsi:type="ecore:EReference" name="nestedUsage" upperBound="-1"
              eType="#//SysML/Systems/DefinitionAndUsage/Usage" derived="true" eOpposite="#//SysML/Systems/DefinitionAndUsage/Usage/owningUsage">
            <eAnnotations source="&lt;p>The &lt;code>Usages&lt;/code> that are &lt;code>ownedFeatures&lt;/code> of this &lt;code>Usage&lt;/code>.&lt;/p>"
                references="#//SysML/Systems/DefinitionAndUsage/Usage/nestedUsage"/>
          </eStructuralFeatures>
          <eStructuralFeatures xsi:type="ecore:EReference" name="nestedReference"
              upperBound="-1" eType="#//SysML/Systems/DefinitionAndUsage/ReferenceUsage"
              derived="true">
            <eAnnotations source="&lt;p>The &lt;code>ReferenceUsages&lt;/code> that are &lt;code>nestedUsages&lt;/code> of this &lt;code>Usage&lt;/code>.&lt;/p>  "
                references="#//SysML/Systems/DefinitionAndUsage/Usage/nestedReference"/>
          </eStructuralFeatures>
          <eStructuralFeatures xsi:type="ecore:EReference" name="nestedAttribute"
              upperBound="-1" eType="#//SysML/Systems/Attributes/AttributeUsage" derived="true">
            <eAnnotations source="&lt;p>The code>AttributeUsages&lt;/code> that are &lt;code>nestedUsages&lt;/code> of this &lt;code>Usage&lt;/code>.&lt;/p> "
                references="#//SysML/Systems/DefinitionAndUsage/Usage/nestedAttribute"/>
          </eStructuralFeatures>
          <eStructuralFeatures xsi:type="ecore:EReference" name="nestedEnumeration"
              upperBound="-1" eType="#//SysML/Systems/Enumerations/EnumerationUsage"
              derived="true">
            <eAnnotations source="&lt;p>The code>EnumerationUsages&lt;/code> that are &lt;code>nestedUsages&lt;/code> of this &lt;code>Usage&lt;/code>.&lt;p>"
                references="#//SysML/Systems/DefinitionAndUsage/Usage/nestedEnumeration"/>
          </eStructuralFeatures>
          <eStructuralFeatures xsi:type="ecore:EReference" name="nestedOccurrence"
              upperBound="-1" eType="#//SysML/Systems/Occurrences/OccurrenceUsage"
              derived="true">
            <eAnnotations source="&lt;p>The &lt;code>OccurrenceUsages&lt;/code> that are &lt;code>nestedUsages&lt;/code> of this &lt;code>Usage&lt;/code>.&lt;/p>"
                references="#//SysML/Systems/DefinitionAndUsage/Usage/nestedOccurrence"/>
          </eStructuralFeatures>
          <eStructuralFeatures xsi:type="ecore:EReference" name="nestedItem" upperBound="-1"
              eType="#//SysML/Systems/Items/ItemUsage" derived="true">
            <eAnnotations source="&lt;p>The &lt;code>ItemUsages&lt;/code> that are &lt;code>nestedUsages&lt;/code> of this &lt;code>Usage&lt;/code>.&lt;/p>"
                references="#//SysML/Systems/DefinitionAndUsage/Usage/nestedItem"/>
          </eStructuralFeatures>
          <eStructuralFeatures xsi:type="ecore:EReference" name="nestedPart" upperBound="-1"
              eType="#//SysML/Systems/Parts/PartUsage" derived="true">
            <eAnnotations source="&lt;p>The &lt;code>PartUsages&lt;/code> that are &lt;code>nestedUsages&lt;/code> of this &lt;code>Usage&lt;/code>.&lt;/p>"
                references="#//SysML/Systems/DefinitionAndUsage/Usage/nestedPart"/>
          </eStructuralFeatures>
          <eStructuralFeatures xsi:type="ecore:EReference" name="nestedPort" upperBound="-1"
              eType="#//SysML/Systems/Ports/PortUsage" derived="true">
            <eAnnotations source="&lt;p>The &lt;code>PortUsages&lt;/code> that are &lt;code>nestedUsages&lt;/code> of this &lt;code>Usage&lt;/code>.&lt;/p>"
                references="#//SysML/Systems/DefinitionAndUsage/Usage/nestedPort"/>
          </eStructuralFeatures>
          <eStructuralFeatures xsi:type="ecore:EReference" name="nestedConnection"
              upperBound="-1" eType="#//SysML/Systems/Connections/ConnectorAsUsage"
              derived="true">
            <eAnnotations source="&lt;p>The &lt;code>ConnectorAsUsages&lt;/code> that are &lt;code>nestedUsages&lt;/code> of this &lt;code>Usage&lt;/code>. Note that this list includes &lt;code>BindingConnectorAsUsages&lt;/code> and &lt;code>SuccessionAsUsages&lt;/code>, even though these are &lt;code>ConnectorAsUsages&lt;/code> but not &lt;code>ConnectionUsages&lt;/code>.&lt;/p>"
                references="#//SysML/Systems/DefinitionAndUsage/Usage/nestedConnection"/>
          </eStructuralFeatures>
          <eStructuralFeatures xsi:type="ecore:EReference" name="nestedFlow" ordered="false"
              upperBound="-1" eType="#//SysML/Systems/Connections/FlowConnectionUsage"
              derived="true">
            <eAnnotations source="&lt;p>The code>FlowConnectionUsages&lt;/code> that are &lt;code>nestedUsages&lt;/code> of this &lt;code>Usage&lt;/code>.&lt;/p>"
                references="#//SysML/Systems/DefinitionAndUsage/Usage/nestedFlow"/>
          </eStructuralFeatures>
          <eStructuralFeatures xsi:type="ecore:EReference" name="nestedInterface"
              upperBound="-1" eType="#//SysML/Systems/Interfaces/InterfaceUsage" derived="true">
            <eAnnotations source="&lt;p>The &lt;code>InterfaceUsages&lt;/code> that are &lt;code>nestedUsages&lt;/code> of this &lt;code>Usage&lt;/code>.&lt;/p>"
                references="#//SysML/Systems/DefinitionAndUsage/Usage/nestedInterface"/>
          </eStructuralFeatures>
          <eStructuralFeatures xsi:type="ecore:EReference" name="nestedAllocation"
              upperBound="-1" eType="#//SysML/Systems/Allocations/AllocationUsage"
              derived="true">
            <eAnnotations source="&lt;p>The &lt;code>AllocationUsages&lt;/code> that are &lt;code>nestedUsages&lt;/code> of this &lt;code>Usage&lt;/code>.&lt;/p>"
                references="#//SysML/Systems/DefinitionAndUsage/Usage/nestedAllocation"/>
          </eStructuralFeatures>
          <eStructuralFeatures xsi:type="ecore:EReference" name="nestedAction" upperBound="-1"
              eType="#//SysML/Systems/Actions/ActionUsage" derived="true">
